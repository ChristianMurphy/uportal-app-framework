          "title": "CareerLocker",
          "description": "Career development resource that includes helpful career information, exploration and planning tools, resume and cover letter programs, and an ePortfolio.",
          "url": "/portal/p/wiscareers/render.uP",
          "iconUrl": null,
          "faIcon": "fa-briefcase",
          "fname": "wiscareers",
          "lifecycleState": "PUBLISHED",
